#include <stdio.h>

/**
 * main - Prints the name of the program.
 *@argc - counts number of arguments
 *@argv - arguments passed from the cli
 * Return: Always 0.
 */
int main(int argc, char *argv[])
{
/* Suppress unused variable warning */
(void)argc;

/* Print the program name */
printf("%s\n", argv[0]);

return (0);
}
